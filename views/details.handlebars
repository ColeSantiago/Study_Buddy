{{#each group}}
	<h1>{{group_name}}</h1>
	{{group_desc}} <br>
{{/each}}

{{#each details}}
	Time: {{grp_date_time}} <br>
	Location: {{grp_location}} <br>
{{/each}}

{{#each group}}
	<button id="join-button" 
	data-userId="{{#each ../user}}{{id}}{{/each}}" 
	data-groupId="{{id}}" 
	class="{{#each ../group_member}}{{is_joined}}{{/each}}" 
	type="submit">
	Join Group!
	</button>	
{{/each}} <br> <br>

{{#each group_member}}
	{{#if is_joined}}
		Joined!
	{{/if}}	
{{/each}}




